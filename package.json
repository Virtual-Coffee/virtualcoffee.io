		"@types/leaflet": "^1.9.12",
		"@types/leaflet.markercluster": "^1.5.4",
		"@types/luxon": "^3.4.2",
		"@types/node": "^20.16.7",
		"@types/react": "npm:types-react@rc",
		"@types/react-dom": "npm:types-react-dom@rc",
		"@types/require-dir": "^1.0.4",
		"@types/sanitize-html": "^2.13.0",
		"all-contributors-cli": "^6.26.1",
		"concurrently": "^9.0.1",
