		"@types/mdx": "^2.0.13",
		"airtable": "^0.12.2",
		"bootstrap": "^4.6.2",
		"calendar-link": "^2.7.0",
		"front-matter": "^4.0.2",
		"graphql": "^16.9.0",
		"graphql-request": "^6.1.0",
		"hast-util-to-string": "^2.0.0",
		"hastscript": "^8.0.0",
		"leaflet": "^1.9.4",
		"leaflet.markercluster": "^1.5.3",
		"luxon": "^3.5.0",
		"markdown-it": "^13.0.2",
		"mdast-util-toc": "^7.1.0",
		"next": "15.4.7",
		"npm-watch": "^0.13.0",
